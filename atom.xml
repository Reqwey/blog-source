&lt;/defs&gt;
&lt;g stroke=&quot;currentColor&quot; fill=&quot;currentColor&quot; stroke-width=&quot;0&quot; transform=&quot;matrix(1 0 0 -1 0 0)&quot; aria-hidden=&quot;true&quot;&gt;
 &lt;use xlink:href=&quot;#E1-MJMAIN-30&quot; x=&quot;0&quot; y=&quot;0&quot;&gt;&lt;/use&gt;
&lt;/g&gt;
&lt;/svg&gt; 条边：&lt;strong&gt;&lt;svg xmlns:xlink=&quot;http://www.w3.org/1999/xlink&quot; width=&quot;1.162ex&quot; height=&quot;2.176ex&quot; style=&quot;vertical-align: -0.338ex;&quot; viewBox=&quot;0 -791.3 500.5 936.9&quot; role=&quot;img&quot; focusable=&quot;false&quot; xmlns=&quot;http://www.w3.org/2000/svg&quot; aria-labelledby=&quot;MathJax-SVG-1-Title&quot;&gt;
&lt;title id=&quot;MathJax-SVG-1-Title&quot;&gt;0&lt;/title&gt;
&lt;defs aria-hidden=&quot;true&quot;&gt;
&lt;path stroke-width=&quot;1&quot; id=&quot;E1-MJMAIN-30&quot; d=&quot;M96 585Q152 666 249 666Q297 666 345 640T423 548Q460 465 460 320Q460 165 417 83Q397 41 362 16T301 -15T250 -22Q224 -22 198 -16T137 16T82 83Q39 165 39 320Q39 494 96 585ZM321 597Q291 629 250 629Q208 629 178 597Q153 571 145 525T137 333Q137 175 145 125T181 46Q209 16 250 16Q290 16 318 46Q347 76 354 130T362 333Q362 478 354 524T321 597Z&quot;&gt;&lt;/path&gt;
&lt;/defs&gt;
&lt;g stroke=&quot;currentColor&quot; fill=&quot;currentColor&quot; stroke-width=&quot;0&quot; transform=&quot;matrix(1 0 0 -1 0 0)&quot; aria-hidden=&quot;true&quot;&gt;
 &lt;use xlink:href=&quot;#E1-MJMAIN-30&quot; x=&quot;0&quot; y=&quot;0&quot;&gt;&lt;/use&gt;
&lt;/g&gt;
&lt;/svg&gt; 要素&lt;/strong&gt;&lt;/p&gt;
&lt;/li&gt;
&lt;li&gt;
&lt;p&gt;每个节点只能与 &lt;svg xmlns:xlink=&quot;http://www.w3.org/1999/xlink&quot; width=&quot;1.162ex&quot; height=&quot;2.176ex&quot; style=&quot;vertical-align: -0.338ex;&quot; viewBox=&quot;0 -791.3 500.5 936.9&quot; role=&quot;img&quot; focusable=&quot;false&quot; xmlns=&quot;http://www.w3.org/2000/svg&quot; aria-labelledby=&quot;MathJax-SVG-1-Title&quot;&gt;
